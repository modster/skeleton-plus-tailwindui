    "@theatre/studio": "^0.7.0",
    "@threlte/core": "^6.0.6",
    "@threlte/extras": "^5.5.1",
    "@threlte/theatre": "^2.1.1",
    "@types/three": "^0.155.1",
    "highlight.js": "11.8.0",
    "three": "^0.156.0",
    "tweakpane": "^4.0.0"
  }
